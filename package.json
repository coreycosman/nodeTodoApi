{
  "name": "nodepractice4-toDoApp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "r": "nodemon server/server.js -e js,hbs",
    "t": "export NODE_ENV=test && nodemon --exec 'mocha test/**/*.test.js'",
    "d": "nodemon --inspect-brk server/server.js",
    "dt": "npm run t -- --inspect-brk",
    "ds": "nodemon inspect server/server.js",
    "start": "node server/server.js",
    "client": "npm run start --prefix server/client",
    "dev": "concurrently \"npm run r\" \"npm run client\"",
    "devd": "concurrently \"npm run d\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
