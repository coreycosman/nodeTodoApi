    "express": "^4.16.3",
    "fs": "^0.0.1-security",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.14",
    "morgan": "^1.9.1",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "redux-logger": "^3.0.6",
    "selenium-standalone": "^6.15.1",
    "socket.io": "^2.1.1",
    "validator": "^10.7.1"
  },
